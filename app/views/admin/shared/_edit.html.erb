<% className = form_action.delete(:class) %>
<%= form_tag(form_action, :id => "#{form_type}_form", :enctype => "multipart/form-data", :class => className) do %>

  <%= render :partial => "form" %>

<% end %>

<%= form_tag('/admin/content/merge/',  :enctype => "multipart/form-data", :class => 'merge') do %>

  <%= render('merges') if (current_user.admin? && !(params[:action] == 'new')) %>

<% end %>


